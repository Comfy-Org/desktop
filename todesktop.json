{
  "$schema": "https://unpkg.com/@todesktop/cli@1.15.2/schemas/schema.json",
  "id": "241012ess7yxs0e",
  "icon": "./assets/UI/Comfy_Logo_x512.png",
  "appBuilderLibVersion": "25.1.8",
  "rebuildLibrary": "@electron/rebuild",
  "schemaVersion": 1,
  "uploadSizeLimit": 800,
  "appPath": ".",
  "appFiles": [
    "src/**",
    "scripts/**",
    "assets/ComfyUI/**",
    "assets/UI/**",
    "assets/requirements/**",
    "assets/uv/**",
    "assets/desktop-ui/**",
    ".vite/**",
    "package.json",
    ".npmrc",
    "pnpm-lock.yaml",
    "pnpm-workspace.yaml",
    ".husky/**"
  ],
  "extraResources": [
    { "from": "./node_modules", "to": "node_modules" },
    { "from": "./assets/desktop-ui", "to": "desktop-ui" },
    { "from": "./assets/ComfyUI", "to": "ComfyUI" },
    { "from": "./assets/requirements", "to": "requirements" },
    { "from": "./assets/override.txt", "to": "override.txt" },
    { "from": "./assets/uv", "to": "uv" },
    { "from": "./assets/UI", "to": "UI" }
  ],
  "filesForDistribution": [
    "!assets/**",
    "!dist/**",
    "!src/**",
    "!scripts/**",
    "!pnpm-lock.yaml",
    "!.husky/**"
  ],
  "mac": {
    "icon": "./assets/UI/Comfy_Logo_x1024.png",
    "additionalBinariesToSign": [
      "./assets/uv/macos/uv",
      "./assets/uv/macos/uvx"
    ]
  },
  "windows": {
    "icon": "./assets/UI/Comfy_Icon_Windows.png",
    "nsisInclude": "./scripts/installer.nsh"
  },
  "updateUrlBase": "https://updater.comfy.org"
}
