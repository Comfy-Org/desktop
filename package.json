{
  "name": "@comfyorg/comfyui-electron",
  "productName": "ComfyUI",
  "repository": "github:comfy-org/electron",
  "copyright": "Copyright Â© 2024 Comfy Org",
  "version": "0.5.5",
  "homepage": "https://comfy.org",
  "description": "The best modular GUI to run AI diffusion models.",
  "main": ".vite/build/main.cjs",
  "packageManager": "pnpm@10.22.0",
  "type": "module",
  "config": {
    "frontend": {
      "version": "1.28.8",
      "optionalBranch": ""
    },
    "comfyUI": {
      "version": "0.3.67",
      "optionalBranch": ""
    },
    "managerCommit": "d82e1f5d677fec0b85a6be6beb1e32d5627ef6af",
    "uvVersion": "0.8.13"
  },
  "scripts": {
    "clean": "rimraf .vite dist out",
    "clean:uv": "rimraf assets/uv",
    "clean:assets": "rimraf assets/.env assets/ComfyUI assets/python.tgz & pnpm run clean:assets:git && pnpm run clean:uv",
    "clean:assets:git": "rimraf assets/ComfyUI/.git assets/ComfyUI/custom_nodes/ComfyUI-Manager/.git",
    "clean:slate": "pnpm run clean & pnpm run clean:assets & rimraf node_modules",
    "download:uv": "node scripts/downloadUV.js",
    "download-frontend": "node scripts/downloadFrontend.js",
    "make:frontend": "pnpm run download-frontend",
    "format": "prettier --write --cache --list-different .",
    "format:check": "prettier --check .",
    "lint": "eslint --cache src",
    "lint:check": "eslint --max-warnings 0 src",
    "lint:fix": "eslint --fix --cache src",
    "typecheck": "tsc --noEmit --strict",
    "make": "pnpm run vite:compile && electron-builder build --config=builder-debug.config.ts && pnpm run verify:build",
    "make:nsis": "electron-builder build --config=builder-debug.config.ts --win nsis --prepackaged ./dist/win-unpacked",
    "make:assets": "node scripts/makeComfy.js",
    "make:nvidia": "pnpm run make -- --nvidia",
    "notarize": "node debug/notarize.js",
    "package": "pnpm run vite:compile && todesktop build --code-sign=false --async",
    "patch:core:frontend": "node scripts/patchComfyUI.js requirements",
    "postinstall": "node .husky/install.mjs",
    "publish": "pnpm run vite:compile && todesktop build",
    "publish:staging": "pnpm run vite:compile && todesktop build --config=./todesktop.staging.json --async",
    "reset-install": "node scripts/resetInstall.js",
    "sign": "node debug/sign.js",
    "start": "vite build --config vite.preload.config.ts && vite build --watch --mode startwatch",
    "test:e2e": "npx playwright test",
    "test:e2e:update": "npx playwright test --update-snapshots",
    "test:unit": "vitest run",
    "todesktop:afterPack": "./scripts/todesktop/afterPack.cjs",
    "todesktop:beforeInstall": "./scripts/todesktop/beforeInstall.cjs",
    "typescript": "tsc -p tsconfig.build.json",
    "verify:build": "node scripts/verifyBuild.js",
    "vite:compile": "pnpm run typescript && vite build && vite build --config vite.preload.config.ts",
    "vite:types": "pnpm run typescript && vite build --config vite.types.config.ts && node scripts/prepareTypes.js",
    "release:types": "node scripts/releaseTypes.js",
    "update:frontend": "node scripts/updateFrontend.js"
  },
  "devDependencies": {
    "@electron/fuses": "^2.0.0",
    "@electron/notarize": "^3.1.1",
    "@electron/rebuild": "^4.0.1",
    "@electron/windows-sign": "^2.0.2",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@sentry/wizard": "^6.6.1",
    "@todesktop/cli": "^1.17.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/node": "^24.10.1",
    "@types/tar": "6.1.13",
    "@types/tmp": "^0.2.6",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "app-builder-lib": "26.0.12",
    "diff": "^8.0.2",
    "dmg-builder": "26.0.12",
    "electron": "39.2.2",
    "electron-builder": "^26.0.12",
    "electron-builder-squirrel-windows": "26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "playwright": "1.56.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.10"
  },
  "keywords": [],
  "author": {
    "name": "Comfy Org",
    "email": "support@comfy.org"
  },
  "license": "GPL-3.0-only",
  "dependencies": {
    "@comfyorg/desktop-ui": "^0.0.3",
    "@sentry/electron": "^7.2.0",
    "@sentry/vite-plugin": "^4.6.0",
    "@todesktop/runtime": "^2.1.3",
    "@types/lodash": "4.17.20",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "electron-log": "^5.4.3",
    "electron-store": "11.0.2",
    "lodash": "4.17.21",
    "mixpanel": "^0.19.1",
    "node-pty": "^1.0.0",
    "systeminformation": "^5.27.11",
    "tar": "^7.5.2",
    "tmp": "^0.2.5",
    "wait-on": "^9.0.3",
    "yaml": "^2.8.1"
  }
}
